{
  publisher <
    name = Youngstown State University Center for Judaic and Holocaust Studies
  >

  plugin = org.lockss.plugin.projmuse.ProjectMusePlugin
  implicit < status ; year ; name ; param[volume] >
  param[base_url] = http://muse.jhu.edu/

  {
    title <
      name = Journal of Jewish Identities ;
      issn = 1946-2522 ;
      eissn = 1939-7941 ;
      issnl = 1939-7941
    >

    param[journal_dir] = journal_of_jewish_identities
    attr[journal_id] = jji

    au < released ; 2008 ; Journal of Jewish Identities Issue 1 ; 1 >
    au < released ; 2009 ; Journal of Jewish Identities Issue 2 ; 2 >
    au < released ; 2010 ; Journal of Jewish Identities Issue 3 ; 3 >
    au < released ; 2011 ; Journal of Jewish Identities Issue 4 ; 4 >
    au < released ; 2012 ; Journal of Jewish Identities Issue 5 ; 5 >

  }

}
