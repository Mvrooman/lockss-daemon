{
  publisher <
    name = Texas State Historical Association
  >

  plugin = org.lockss.plugin.projmuse.ProjectMusePlugin
  implicit < status ; year ; name ; param[volume] >
  param[base_url] = http://muse.jhu.edu/

  {

    title <
      name = Southwestern Historical Quarterly ;
      issn = 0038-478X ;
      eissn = 1558-9560 ;
      issnl = 0038-478X
    >

    param[journal_dir] = southwestern_historical_quarterly
    attr[journal_id] = swh

    au < released ; 2006-2007 ; Southwestern Historical Quarterly Volume 110 ; 110 >
    au < released ; 2007-2008 ; Southwestern Historical Quarterly Volume 111 ; 111 >
    au < released ; 2008-2009 ; Southwestern Historical Quarterly Volume 112 ; 112 >
    au < released ; 2009-2010 ; Southwestern Historical Quarterly Volume 113 ; 113 >
    au < released ; 2010-2011 ; Southwestern Historical Quarterly Volume 114 ; 114 >
    au < released ; 2011-2012 ; Southwestern Historical Quarterly Volume 115 ; 115 >
    au < ready ; 2012-2013 ; Southwestern Historical Quarterly Volume 116 ; 116 >

  }

}
