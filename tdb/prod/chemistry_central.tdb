{

  publisher <
    name = Chemistry Central
  >
  
  rights = openaccess
  plugin = org.lockss.plugin.bmc.BioMedCentralPlugin
  implicit < status ; year ; name ; param[volume_name] >
  
  {
  
    title <
      name = Chemistry Central Journal ;
      issn = 1752-153X ;
      issnl = 1752-153X
    >

    param[journal_issn] = 1752-153X
    
    {
      param[base_url] = http://www.journal.chemistrycentral.com/
      param[journal_issn] = 1752-153X
      
      au < down ; 2007 ; Chemistry Central Journal Volume 1 [superseded] ; 1 >
      au < down ; 2008 ; Chemistry Central Journal Volume 2 [superseded] ; 2 >
      au < down ; 2009 ; Chemistry Central Journal Volume 3 [superseded] ; 3 >
      au < down ; 2010 ; Chemistry Central Journal Volume 4 [superseded] ; 4 >
      
    }
    
    {
      param[base_url] = http://journal.chemistrycentral.com/
      param[journal_issn] = 1752-153X
      
      au < doNotProcess ; 2007 ; Chemistry Central Journal Volume 1 ; 1 >
      au < doNotProcess ; 2008 ; Chemistry Central Journal Volume 2 ; 2 >
      au < doNotProcess ; 2009 ; Chemistry Central Journal Volume 3 ; 3 >
      au < doNotProcess ; 2010 ; Chemistry Central Journal Volume 4 ; 4 >
      au < manifest ; 2011 ; Chemistry Central Journal Volume 5 ; 5 >
      au < manifest ; 2012 ; Chemistry Central Journal Volume 6 ; 6 >
      
    }
  
  }
  
  {
  
    title <
      name = Journal of Cheminformatics ;
      issn = 1758-2946 ;
      issnl = 1758-2946
    >

    param[base_url] = http://www.jcheminf.com/
    param[journal_issn] = 1758-2946
    
    au < released ; 2009 ; Journal of Cheminformatics Volume 1 ; 1 >
    au < released ; 2010 ; Journal of Cheminformatics Volume 2 ; 2 >
    au < manifest ; 2011 ; Journal of Cheminformatics Volume 3 ; 3 >
    au < manifest ; 2012 ; Journal of Cheminformatics Volume 4 ; 4 >

  }
  
  {
  
    title <
      name = Journal of Systems Chemistry ;
      issn = 1759-2208 ;
      issnl = 1759-2208
    >
    
    param[base_url] = http://www.jsystchem.com/
    param[journal_issn] = 1759-2208
    
    au < manifest ; 2010 ; Journal of Systems Chemistry Volume 1 ; 1 >
    au < released ; 2011 ; Journal of Systems Chemistry Volume 2 ; 2 >
    au < released ; 2012 ; Journal of Systems Chemistry Volume 3 ; 3 >

  }
  
}
